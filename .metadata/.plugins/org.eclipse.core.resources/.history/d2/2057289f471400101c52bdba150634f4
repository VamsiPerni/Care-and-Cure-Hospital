			    <!-- JavaScript for Active Navigation Links -->
			    <script>
			        document.querySelectorAll('.navbar a').forEach(link => {
			            link.addEventListener('click', function() {
			                document.querySelectorAll('.navbar a').forEach(a => a.classList.remove('active'));
			                this.classList.add('active');
			            });
			        });

			        // JavaScript for Pricing Section Scroll
			        function scrollPricing(direction) {
			            const container = document.querySelector('.pricing .pricing-container');
			            const scrollAmount = 300; // Adjust this value to control the scroll amount
			            container.scrollBy({ left: direction * scrollAmount, behavior: 'smooth' });
			        }

			        // JavaScript for Doctors Section Scroll
			        function scrollDoctors(index) {
			            const container = document.querySelector('.doctors .doctors-container');
			            const doctorBoxWidth = document.querySelector('.doctor-box').offsetWidth + 20; // Width of one doctor box + gap
			            container.scrollTo({ left: index * doctorBoxWidth, behavior: 'smooth' });

			            // Update active dot
			            document.querySelectorAll('.doctors .dot').forEach((dot, i) => {
			                dot.classList.toggle('active', i === index);
			            });
			        }
			    </script>
			</body>
			</html>